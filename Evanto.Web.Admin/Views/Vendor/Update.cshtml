<form id="resetPassword" method="post">
    <!-- Modal -->
    <div id="myModal" class="modal fade" style="margin-top: 200px;">
        <div class="modal-dialog">
            <div class="modal-content" style="height: 300px">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    <h4 class="modal-title">Reset Password</h4>
                </div>
                <div class="modal-body" style="height: 150px">
                    <input type="hidden" name="Id" value="@Model.Id" />
                    <div style="height: 70px">

                        <div>
                            <div class="uk-input-group">
                                <span class="uk-input-group-addon">
                                    <i class="md-list-addon-icon material-icons md-36 uk-text-danger">&#xE851;</i>
                                </span>
                                <label>User Role</label>
                                @Html.DropDownListFor(x => x.RoleId, @Model.Roles, "Select...", new { data_md_selectize = "data_md_selectize", })
                            </div>
                            <button type="button" onclick="addtoList()"> add to List</button>
                        </div>
                        <div>
                            <ul id="serviceList"></ul>
                        </div>
                        @*<label> New Password</label>
                            <input type="password" id="NewPassword" name="NewPassword" class="md-input validate[required,maxSize[50]]"/>*@
                    </div>


                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                    <button type="submit" class="btn btn-primary" formaction="@Url.Action("ResetPassword","Vendor")">Save changes</button>
                </div>
            </div>
        </div>
    </div>
</form>

