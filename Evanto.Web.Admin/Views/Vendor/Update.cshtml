                    <input type="text" name="BookDate" disabled="disabled" id="bookDay" />
                </div>
                <div class="modal-body">

                    <div class="uk-margin-medium-bottom" id="calendar_colors">

                        <div class="uk-row-first">

                            <div class="md-card" style="color: black">
                                <div class="md-card-content">
                                    <div class="uk-grid">
                                        <div class="uk-width-large-2-3 uk-width-1-1">
                                            <div class="uk-input-group">
                                                <span class="uk-input-group-addon"><i class="uk-input-group-icon uk-icon-list"></i></span>
                                                <div class="md-input-wrapper">

                                                    <select name="VendorServiceId" class="md-input validate[required]">
                                                        @for (int i = 0; i < @Model.VendorServicePackets.Count; i++)
                                                        {
                                                            if (@Model.VendorServicePackets[i].VendorService.Count() > 0)
                                                            {
                                                                //<optgroup label="@Model.VendorServicePackets[i].Id-@Model.VendorServicePackets[i].CreatedDate">
                                                                foreach (var item in Model.VendorServicePackets[i].VendorService.Distinct())
                                                                {
                                                                    if (@item.Status && !String.IsNullOrEmpty(@item.Name.Trim()))
                                                                    {
                                                                <option value="@item.Id">@item.Name</option>
                                                                    }

                                                                }
                                                                // </optgroup>
                                                            }

                                                        }
                                                    </select>
                                                    <span class="md-input-bar "></span>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

