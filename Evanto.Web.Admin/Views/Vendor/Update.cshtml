        function ChangeStatus(thisButton,id, status) {
            var $this = thisButton;
            $.ajax({
                url: '/Vendor/ChangeServiceStatus?Id='+id+"&Status="+status,
                type: 'get',
                dataType: 'json',
                success: function (data) {
                    console.log(data);
                    var message;
                    if (data.Status == true) {
                        thisButton.innerText = "Deactivate";

                        message = "<a href='#' class='notify-action'>Clear</a> Service Status Activated";

                    } else {
                        thisButton.innerText = "Activate";
                        message = "<a href='#' class='notify-action'>Clear</a> Service Status Deactivated";

                    }

                    var status = "success";
                    var pos = "top-right";
                    showNotification(message, status, pos);
                    setTimeout('location.reload()', 2000);

                },
                error: function () {
                }
            });

        };

